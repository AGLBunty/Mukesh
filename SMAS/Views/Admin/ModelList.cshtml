			<div class="runningObsoleteTab">
				<ul class="nav nav-tabs" id="myTab" role="tablist">
					<li class="nav-item">
						<a class="nav-link active" id="running-tab" data-toggle="tab" href="#running" role="tab" aria-controls="running" aria-selected="true" onclick="fn_runningdata();">Running</a>
					</li>
					<li class="nav-item">
						<a class="nav-link" id="obsolete-tab" data-toggle="tab" href="#obsolete" role="tab" aria-controls="obsolete" aria-selected="false" onclick="fn_obsoletedata();">Obsolete</a>
					</li>
				</ul>
				<div class="tab-content" id="myTabContent">
					<div class="tab-pane fade show active" id="running" role="tabpanel" aria-labelledby="running-tab">
						<div class="tabContentMain">
							<div class="tabContentTop">
								<h2>Browse by models</h2>
								<div class="searchMain">
									<div class="searchInput">
										<input type="text" id="txtrunningSearch" name="" placeholder="Search">
                                        <input type="hidden" id="hid_lid" name="hid_lid" value="@ViewBag.Lid" />
									</div>
								</div>
							</div>
							<div><input type="hidden" id="domainname" name="domainname" value="@SMAS.Models.clsBLLHttpUtility.GetDomin()" /></div>
							<div class="modelBoxMain" id="runningdivdata">
							
							</div>
						</div>
					</div>
					<div class="tab-pane fade" id="obsolete" role="tabpanel" aria-labelledby="obsolete-tab">
						<div class="tabContentMain">
							<div class="tabContentTop">
								<h2>Browse by models</h2>
								<div class="searchMain">
									@*<div class="uploadManualLink actionUpLink"><a href="#">Upload Manual</a></div>*@
									<div class="searchInput">
										<input type="text" name="" id="txtobsoleteSearch" placeholder="Search">
									</div>
								</div>
							</div>
							<div class="modelBoxMain" id="obsoletedivdata">
							
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

